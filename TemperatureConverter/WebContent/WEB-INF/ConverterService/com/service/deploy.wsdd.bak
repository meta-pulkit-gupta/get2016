<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ConverterService WSDL service -->

  <service name="Converter" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.com"/>
      <parameter name="wsdlServiceElement" value="ConverterService"/>
      <parameter name="schemaQualified" value="http://service.com"/>
      <parameter name="wsdlServicePort" value="Converter"/>
      <parameter name="className" value="com.service.ConverterSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Converter"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="fahrenhietToCelcius" qname="operNS:fahrenhietToCelcius" xmlns:operNS="http://service.com" returnQName="retNS:fahrenhietToCelciusReturn" xmlns:retNS="http://service.com" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:fahrenhiet" xmlns:pns="http://service.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="fahrenhietToCelcius"/>

  </service>
</deployment>
